&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; (&lt;span style="color:#e6db74"&gt;&amp;#39;7&amp;#39;&lt;/span&gt;, &lt;span style="color:#e6db74"&gt;&amp;#39;2&amp;#39;&lt;/span&gt;, &lt;span style="color:#e6db74"&gt;&amp;#39;2020-01-08 21:30:45&amp;#39;&lt;/span&gt;, &lt;span style="color:#e6db74"&gt;&amp;#39;25km&amp;#39;&lt;/span&gt;, &lt;span style="color:#e6db74"&gt;&amp;#39;25mins&amp;#39;&lt;/span&gt;, &lt;span style="color:#e6db74"&gt;&amp;#39;null&amp;#39;&lt;/span&gt;),
